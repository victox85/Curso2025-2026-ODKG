@prefix base: <http://crtm-urban-buses.org/opendata/handsOn/group11> .
@prefix ont: <http://crtm-urban-buses.org/opendata/handsOn/group11/ontology#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix mod: <http://w3id.org/mod#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

base: a owl:Ontology ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

ont:Agency a owl:Class ;
  rdfs:label "Agency" ;
  rdfs:subClassOf schema:Organization .

ont:BusRoute a owl:Class ;
  rdfs:label "Bus Route" ;
  rdfs:subClassOf schema:BusRoute .

ont:Trip a owl:Class ;
  rdfs:label "Trip" ;
  rdfs:subClassOf schema:Trip .

ont:Stop a owl:Class ;
  rdfs:label "Stop" ;
  rdfs:subClassOf schema:Place .

ont:StopTime a owl:Class ;
  rdfs:label "Stop Time" ;
  rdfs:subClassOf schema:Event .

ont:CalendarService a owl:Class ;
  rdfs:label "Calendar Service" ;
  rdfs:subClassOf time:TemporalEntity .

ont:Shape a owl:Class ;
  rdfs:label "Shape" ;
  rdfs:subClassOf schema:Geometry .

[] a owl:AllDisjointClasses ;
   owl:members ( ont:Agency ont:BusRoute ont:Trip ont:Stop ont:StopTime ont:CalendarService ont:Shape ) .

ont:operatedBy a owl:ObjectProperty ;
  rdfs:label "operated by" ;
  rdfs:domain ont:BusRoute ;
  rdfs:range  ont:Agency .

ont:operatesRoute a owl:ObjectProperty ;
  rdfs:label "operates route" ;
  rdfs:domain ont:Agency ;
  rdfs:range  ont:BusRoute ;
  owl:inverseOf ont:operatedBy .

ont:hasTrip a owl:ObjectProperty ;
  rdfs:label "has trip" ;
  rdfs:domain ont:BusRoute ;
  rdfs:range  ont:Trip .

ont:belongsToRoute a owl:ObjectProperty ;
  rdfs:label "belongs to route" ;
  rdfs:domain ont:Trip ;
  rdfs:range  ont:BusRoute ;
  owl:inverseOf ont:hasTrip .

ont:hasStopTime a owl:ObjectProperty ;
  rdfs:label "has stop time" ;
  rdfs:domain ont:Trip ;
  rdfs:range  ont:StopTime .

ont:belongsToTrip a owl:ObjectProperty ;
  rdfs:label "belongs to trip" ;
  rdfs:domain ont:StopTime ;
  rdfs:range  ont:Trip ;
  owl:inverseOf ont:hasStopTime .

ont:refersToStop a owl:ObjectProperty ;
  rdfs:label "refers to stop" ;
  rdfs:domain ont:StopTime ;
  rdfs:range  ont:Stop .

ont:parentStation a owl:ObjectProperty ;
  rdfs:label "parent station" ;
  rdfs:domain ont:Stop ;
  rdfs:range  ont:Stop .

ont:hasChildStop a owl:ObjectProperty ;
  rdfs:label "has child stop" ;
  rdfs:domain ont:Stop ;
  rdfs:range  ont:Stop ;
  owl:inverseOf ont:parentStation .

ont:hasCalendar a owl:ObjectProperty ;
  rdfs:label "has calendar" ;
  rdfs:domain ont:Trip ;
  rdfs:range  ont:CalendarService .

ont:isCalendarOf a owl:ObjectProperty ;
  rdfs:label "is calendar of" ;
  rdfs:domain ont:CalendarService ;
  rdfs:range  ont:Trip ;
  owl:inverseOf ont:hasCalendar .

ont:hasShape a owl:ObjectProperty ;
  rdfs:label "has shape" ;
  rdfs:domain ont:Trip ;
  rdfs:range  ont:Shape .

ont:isShapeOf a owl:ObjectProperty ;
  rdfs:label "is shape of" ;
  rdfs:domain ont:Shape ;
  rdfs:range  ont:Trip ;
  owl:inverseOf ont:hasShape .

ont:nextStop a owl:ObjectProperty ;
  rdfs:label "next stop" ;
  rdfs:domain ont:Stop ;
  rdfs:range  ont:Stop .

ont:agencyId a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:label "agency id" ;
  rdfs:domain ont:Agency ;
  rdfs:range xsd:string .

ont:timezone a owl:DatatypeProperty ;
  rdfs:label "timezone" ;
  rdfs:domain ont:Agency ;
  rdfs:range xsd:string .

ont:fareUrl a owl:DatatypeProperty ;
  rdfs:label "fare url" ;
  rdfs:domain ont:Agency ;
  rdfs:range xsd:anyURI .

ont:routeId a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:label "route id" ;
  rdfs:domain ont:BusRoute ;
  rdfs:range xsd:string .

ont:agencyId a owl:DatatypeProperty ;
  rdfs:label "agency id" ;
  rdfs:domain ont:BusRoute ;
  rdfs:range xsd:string .

ont:routeShortName a owl:DatatypeProperty ;
  rdfs:label "route short name" ;
  rdfs:domain ont:BusRoute ;
  rdfs:range xsd:string .

ont:routeLongName a owl:DatatypeProperty ;
