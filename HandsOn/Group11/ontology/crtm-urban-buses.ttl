  rdfs:range xsd:string .

ont:routeType a owl:DatatypeProperty ;
  rdfs:label "route type" ;
  rdfs:domain ont:BusRoute ;
  rdfs:range xsd:integer .

ont:routeColor a owl:DatatypeProperty ;
  rdfs:label "route color" ;
  rdfs:domain ont:BusRoute ;
  rdfs:range xsd:string .

ont:routeTextColor a owl:DatatypeProperty ;
  rdfs:label "route text color" ;
  rdfs:domain ont:BusRoute ;
  rdfs:range xsd:string .

ont:tripId a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:label "trip id" ;
  rdfs:domain ont:Trip ;
  rdfs:range xsd:string .

ont:tripShortName a owl:DatatypeProperty ;
  rdfs:label "trip short name" ;
  rdfs:domain ont:Trip ;
  rdfs:range xsd:string .

ont:directionId a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:label "direction id" ;
  rdfs:domain ont:Trip ;
  rdfs:range xsd:string .

ont:wheelChairAccessible a owl:DatatypeProperty ;
  rdfs:label "wheelchair accessible" ;
  rdfs:domain ont:Trip ;
  rdfs:range xsd:integer .

ont:arrivalTime a owl:DatatypeProperty ;
  rdfs:label "arrival time" ;
  rdfs:domain ont:StopTime ;
  rdfs:range xsd:string .

ont:departureTime a owl:DatatypeProperty ;
  rdfs:label "departure time" ;
  rdfs:domain ont:StopTime ;
  rdfs:range xsd:string .

ont:stopSequence a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:label "stop sequence" ;
  rdfs:domain ont:StopTime ;
  rdfs:range xsd:integer .

ont:pickupType a owl:DatatypeProperty ;
  rdfs:label "pickup type" ;
  rdfs:domain ont:StopTime ;
  rdfs:range xsd:integer .

ont:dropOffType a owl:DatatypeProperty ;
  rdfs:label "drop off type" ;
  rdfs:domain ont:StopTime ;
  rdfs:range xsd:integer .

ont:stopShapeDistTraveled a owl:DatatypeProperty ;
  rdfs:label "stop shape distance traveled" ;
  rdfs:domain ont:StopTime ;
  rdfs:range xsd:decimal .

ont:stopId a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:label "stop id" ;
  rdfs:domain ont:Stop ;
  rdfs:range xsd:string .

ont:stopCode a owl:DatatypeProperty ;
  rdfs:label "stop code" ;
  rdfs:domain ont:Stop ;
  rdfs:range xsd:string .

ont:zoneId a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:label "zone id" ;
  rdfs:domain ont:Stop ;
  rdfs:range xsd:string .

ont:locationType a owl:DatatypeProperty ;
  rdfs:label "location type" ;
  rdfs:domain ont:Stop ;
  rdfs:range xsd:integer .

ont:wheelChairBoarding a owl:DatatypeProperty ;
  rdfs:label "wheelchair boarding" ;
  rdfs:domain ont:Stop ;
  rdfs:range xsd:integer .

