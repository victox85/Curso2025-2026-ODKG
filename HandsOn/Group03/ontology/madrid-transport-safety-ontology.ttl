@prefix : <http://madrid-transport-safety.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <http://schema.org/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

@base <http://madrid-transport-safety.org/ontology> .

#    Ontology Declaration

<http://madrid-transport-safety.org/ontology> rdf:type owl:Ontology ;
    dcterms:title "Madrid Transportation and Safety Ontology"@en ;
    dcterms:description "An ontology for representing transportation infrastructure and safety data in Madrid, including bicycle accidents, traffic lights, pedestrian data, and bicycle infrastructure data."@en ;
    dcterms:creator "Group 03" ;
    dcterms:created "2025-10-05"^^xsd:date ;
    dcterms:modified "2025-10-10"^^xsd:date ;
    dcterms:license <http://creativecommons.org/licenses/by/4.0/> ;
    dcterms:source "Ayuntamiento de Madrid - Open Data Portal" ;
    owl:versionInfo "2.0" ;
    rdfs:comment "This ontology integrates data from multiple Madrid city datasets to support analysis of urban transportation safety and infrastructure. Updated to include enhanced properties for accident details, person information, traffic light specifications, and measurement data."@en .


#    Classes

###  http://madrid-transport-safety.org/ontology#Accident
:Accident rdf:type owl:Class ;
    rdfs:subClassOf schema:Event ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :hasLocation ;
          owl:someValuesFrom :Location
        ] ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :accidentDate ;
          owl:someValuesFrom xsd:dateTime
        ] ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :vehicleType ;
          owl:someValuesFrom xsd:string
        ] ;
    rdfs:label "Bicycle Accident"@en , "Accidente de Bicicleta"@es ;
    rdfs:comment "Represents an accident involving a bicycle in Madrid."@en ;
    skos:definition "An event where a bicycle was involved in a traffic accident."@en .

###  http://madrid-transport-safety.org/ontology#TrafficLight
:TrafficLight rdf:type owl:Class ;
    rdfs:subClassOf
        [ rdf:type owl:Restriction ;    # chiedere cosa è
          owl:onProperty :hasLocation ;
          owl:someValuesFrom :Location
        ] ;
    rdfs:label "Traffic Light"@en , "Semáforo"@es ;
    rdfs:comment "Represents a traffic light specifically for bicycle traffic in Madrid."@en ;
    skos:definition "A traffic signal device installed for controlling bicycle traffic flow."@en .

###  http://madrid-transport-safety.org/ontology#PedestrianData
:PedestrianData rdf:type owl:Class ;
    rdfs:subClassOf schema:Event ;
    rdfs:label "Pedestrian Data"@en , "Datos de Peatones"@es ;
    rdfs:comment "Represents data related to pedestrian activity or incidents in Madrid."@en ;
    skos:definition "Information about pedestrian movement, counts, or incidents."@en .

###  http://madrid-transport-safety.org/ontology#BicycleStation
:BicycleStation rdf:type owl:Class ;
    rdfs:subClassOf
        [ rdf:type owl:Restriction ;
          owl:onProperty :hasLocation ;
          owl:someValuesFrom :Location
        ] ;
    rdfs:label "Bicycle Station"@en , "Estación de Bicicletas"@es ;
    rdfs:comment "Represents a bicycle counting device or measurement point in Madrid."@en ;
    skos:definition "A device or measurement point for bicycle traffic monitoring and counting."@en .

###  http://madrid-transport-safety.org/ontology#Location
:Location rdf:type owl:Class ;
    rdfs:subClassOf schema:Place ,
        geo:Feature ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :latitude ;
          owl:someValuesFrom xsd:decimal
        ] ,
        [ rdf:type owl:Restriction ;
          owl:onProperty :longitude ;
          owl:someValuesFrom xsd:decimal
        ] ;
    rdfs:label "Location"@en , "Ubicación"@es ;
    rdfs:comment "Represents a geographical location in Madrid."@en ;
    skos:definition "A specific point in geographical space defined by coordinates."@en .

###  http://madrid-transport-safety.org/ontology#District
:District rdf:type owl:Class ;
    rdfs:subClassOf schema:AdministrativeArea ;
    rdfs:label "District"@en , "Distrito"@es ;
    rdfs:comment "Represents an administrative district of Madrid."@en ;
    skos:definition "One of the 21 administrative districts that make up the city of Madrid."@en .

###  http://madrid-transport-safety.org/ontology#Street
:Street rdf:type owl:Class ;
    rdfs:subClassOf schema:Place ;
    rdfs:label "Street"@en , "Calle"@es ;
    rdfs:comment "Represents a street in Madrid."@en ;
    skos:definition "A public road in an urban area."@en .

###  http://madrid-transport-safety.org/ontology#Person
:Person rdf:type owl:Class ;
    rdfs:subClassOf schema:Person ;
    rdfs:label "Person"@en , "Persona"@es ;
    rdfs:comment "Represents a person involved in an accident."@en ;
    skos:definition "An individual involved in a traffic incident."@en .


#    Object Properties

###  http://madrid-transport-safety.org/ontology#hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
    rdfs:domain owl:Thing ;
    rdfs:range :Location ;
    rdfs:label "has location"@en , "tiene ubicación"@es ;
    rdfs:comment "Relates an entity to its geographical location."@en ;
    skos:definition "Links an object to the place where it is situated."@en .

###  http://madrid-transport-safety.org/ontology#hasDistrict
:hasDistrict rdf:type owl:ObjectProperty ;
    rdfs:domain :Location ;
    rdfs:range :District ;
    rdfs:label "has district"@en , "tiene distrito"@es ;
    rdfs:comment "Associates a location with an administrative district."@en ;
    skos:definition "Indicates which district a location belongs to."@en .

###  http://madrid-transport-safety.org/ontology#hasStreet
:hasStreet rdf:type owl:ObjectProperty ;
    rdfs:domain :Location ;
    rdfs:range :Street ;
    rdfs:label "has street"@en , "tiene calle"@es ;
    rdfs:comment "Associates a location with a street."@en ;
    skos:definition "Indicates which street a location is on."@en .

###  http://madrid-transport-safety.org/ontology#involvesPedestrian
:involvesPedestrian rdf:type owl:ObjectProperty ;
    rdfs:domain :Accident ;
    rdfs:range :PedestrianData ;
    rdfs:label "involves pedestrian"@en , "involucra peatón"@es ;
    rdfs:comment "Indicates that an accident involved a pedestrian."@en ;
    skos:definition "Links an accident to pedestrian involvement data."@en .

###  http://madrid-transport-safety.org/ontology#locatedInDistrict
:locatedInDistrict rdf:type owl:ObjectProperty ;
    rdfs:domain owl:Thing ;
    rdfs:range :District ;
    rdfs:label "located in district"@en , "ubicado en distrito"@es ;
    rdfs:comment "Direct relationship between an entity and its district."@en ;
    skos:definition "Specifies the district where something is located."@en .

###  http://madrid-transport-safety.org/ontology#nearTrafficLight
:nearTrafficLight rdf:type owl:ObjectProperty ;
    rdfs:domain :Accident ;
    rdfs:range :TrafficLight ;
    rdfs:label "near traffic light"@en , "cerca de semáforo"@es ;
    rdfs:comment "Indicates an accident occurred near a traffic light."@en ;
    skos:definition "Spatial relationship indicating proximity to a traffic light."@en .

###  http://madrid-transport-safety.org/ontology#involvesPerson
:involvesPerson rdf:type owl:ObjectProperty ;
    rdfs:domain :Accident ;
    rdfs:range :Person ;
    rdfs:label "involves person"@en , "involucra persona"@es ;
    rdfs:comment "Indicates that a person was involved in an accident."@en ;
    skos:definition "Links an accident to a person involved in it."@en .


#    Data Properties

###  http://madrid-transport-safety.org/ontology#accidentDate
:accidentDate rdf:type owl:DatatypeProperty ;
    rdfs:domain :Accident ;
    rdfs:range xsd:dateTime ;
    rdfs:label "accident date"@en , "fecha del accidente"@es ;
    rdfs:comment "The date and time when an accident occurred."@en ;
    skos:definition "Temporal information about when an accident took place."@en .

###  http://madrid-transport-safety.org/ontology#latitude
:latitude rdf:type owl:DatatypeProperty ;
    rdfs:domain :Location ;
    rdfs:range xsd:decimal ;
    rdfs:label "latitude"@en ,
               "latitud"@es ;
    rdfs:comment "The latitude coordinate of a location."@en ;
    skos:definition "The angular distance of a place north or south of the earth's equator."@en .

###  http://madrid-transport-safety.org/ontology#longitude
:longitude rdf:type owl:DatatypeProperty ;
    rdfs:domain :Location ;
    rdfs:range xsd:decimal ;
    rdfs:label "longitude"@en , "longitud"@es ;
    rdfs:comment "The longitude coordinate of a location."@en ;
    skos:definition "The angular distance of a place east or west of the meridian at Greenwich, England."@en .

