@prefix : <https://iamabsa.github.io/chargingstation/> .
@prefix ns: <https://iamabsa.github.io/chargingstation#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix mod: <https://w3id.org/mod#> .
@prefix hasID xsd: <http://www.owl-ontologies.com/hasID xsd#> .
@prefix hasAddress xsd: <http://www.owl-ontologies.com/hasAddress xsd#> .
@prefix directPayment xsd: <http://www.owl-ontologies.com/directPayment xsd#> .
@prefix hasEuId xsd: <http://www.owl-ontologies.com/hasEuId xsd#> .
@prefix geoLoc xsd: <http://www.owl-ontologies.com/geoLoc xsd#> .
@prefix countryCode xsd: <http://www.owl-ontologies.com/countryCode xsd#> .
@base <https://iamabsa.github.io/chargingstation/> .

<https://iamabsa.github.io/chargingstation/> rdf:type owl:Ontology ;
			mod:createdWith <https://chowlk.linkeddata.es/> .

#################################
#    Object Properties
#################################

## ns:isInDistrict
ns:isInDistrict rdf:type owl:ObjectProperty ;
		rdfs:domain ns:Class1 ;
		rdfs:label "is in district" .

## ns:districtCity
ns:districtCity rdf:type owl:ObjectProperty ;
		rdfs:domain ns:District ;
		rdfs:range ns:City ;
		rdfs:label "district city" .

## ns:isOperatedBy
ns:isOperatedBy rdf:type owl:ObjectProperty ;
		rdfs:domain ns:ChargingStation ;
		rdfs:label "is operated by" .

## ns:isCityOf
ns:isCityOf rdf:type owl:ObjectProperty ;
		rdfs:label "is city of" .

#################################
#    Data Properties
#################################

## <ns>
<ns> rdf:type owl:DatatypeProperty ;
		rdfs:domain ns:ChargingStation ;
		rdfs:range hasID xsd:String ;
		rdfs:label "ns" .

## <ns>
<ns> rdf:type owl:DatatypeProperty ;
		rdfs:domain ns:ChargingStation ;
		rdfs:range hasAddress xsd:String ;
		rdfs:label "ns" .

## <ns>
<ns> rdf:type owl:DatatypeProperty ;
		rdfs:domain ns:ChargingStation ;
		rdfs:range directPayment xsd:Integer ;
		rdfs:label "ns" .

## <ns>
<ns> rdf:type owl:DatatypeProperty ;
		rdfs:domain ns:ChargingStation ;
		rdfs:range hasEuId xsd:string ;
		rdfs:label "ns" .

## <ns>
<ns> rdf:type owl:DatatypeProperty ;
		rdfs:domain ns:ChargingStation ;
		rdfs:range geoLoc xsd:string ;
		rdfs:label "ns" .

## <ns>
<ns> rdf:type owl:DatatypeProperty ;
		rdfs:domain ns:Country ;
		rdfs:range countryCode xsd:Integer ;
		rdfs:label "ns" .

#################################
#    Classes
#################################

## ns:ChargingStation
ns:ChargingStation rdf:type owl:Class ;
	rdfs:label "Charging Station" .

## ns:Operator
ns:Operator rdf:type owl:Class ;
	rdfs:label "Operator" .

## ns:City
ns:City rdf:type owl:Class ;
	rdfs:label "City" .

## ns:District
ns:District rdf:type owl:Class ;
	rdfs:label "District" .

## ns:Country
ns:Country rdf:type owl:Class ;
	rdfs:label "Country" .

