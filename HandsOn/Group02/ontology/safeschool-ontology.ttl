@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema:  <https://schema.org/> .
@prefix wgs84:   <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix safeschool: <http://safeschool.linkeddata.es/ontology#> .

<http://safeschool.linkeddata.es/ontology> a owl:Ontology ;
  dcterms:title "SafeSchool Madrid Ontology"@en ;
  dcterms:description "Ontology for linking Madrid traffic accidents, educational centers, and fixed speed cameras for school safety zone analysis. Aligned with Schema.org, WGS84, and Dublin Core Terms."@en ;
  dcterms:creator "Hands-On Group 02" ;
  dcterms:created "2024-10-06"^^xsd:date ;
  dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
  dcterms:source <https://datos.gob.es/es/catalogo/a13002908-centros-educativos-de-la-comunidad-de-madrid1> ,
                 <https://datos.madrid.es/portal/site/egob/> ;
  owl:versionInfo "1.0.0" .

##########
# CLASSES
##########

safeschool:Accident a owl:Class ;
  rdfs:label "Traffic Accident"@en ;
  rdfs:subClassOf schema:Event ;
  rdfs:comment "A traffic accident registered in Madrid"@en .

safeschool:EducationalCenter a owl:Class ;
  rdfs:label "Educational Center"@en ;
  rdfs:subClassOf schema:EducationalOrganization ;
  rdfs:comment "An educational institution in the Community of Madrid"@en .

safeschool:FixedSpeedCamera a owl:Class ;
  rdfs:label "Fixed Speed Camera"@en ;
  rdfs:subClassOf schema:Place ;
  rdfs:comment "A fixed speed monitoring camera or radar in Madrid"@en .

safeschool:Location a owl:Class ;
  rdfs:label "Geographic Location"@en ;
  rdfs:comment "Geographic coordinates"@en .

safeschool:InvolvedParty a owl:Class ;
  rdfs:label "Involved Party"@en ;
  rdfs:subClassOf schema:Person ;
  rdfs:comment "A person involved in a traffic accident"@en .

safeschool:District a owl:Class ;
  rdfs:label "District"@en ;
  rdfs:subClassOf schema:AdministrativeArea ;
  rdfs:comment "An administrative district within Madrid city"@en .

###################
# OBJECT PROPERTIES
###################

safeschool:locatedAt a owl:ObjectProperty ;
  rdfs:label "located at"@en ;
  rdfs:domain owl:Thing ;
  rdfs:range safeschool:Location ;
  rdfs:comment "Links an entity to its geographic location with coordinates"@en .

safeschool:inDistrict a owl:ObjectProperty ;
  rdfs:label "in district"@en ;
  rdfs:subPropertyOf schema:containedInPlace ;
  rdfs:domain schema:Place ;
  rdfs:range safeschool:District ;
  rdfs:comment "The district where the entity is located (Madrid city only)"@en .

safeschool:hasInvolvedParty a owl:ObjectProperty ;
  rdfs:label "has involved party"@en ;
  owl:equivalentProperty schema:participant ;
  rdfs:domain safeschool:Accident ;
  rdfs:range safeschool:InvolvedParty ;
  rdfs:comment "A person involved in the accident"@en .

###########################################
# DATATYPE PROPERTIES - ACCIDENT
###########################################

safeschool:accidentId a owl:DatatypeProperty ;
  rdfs:label "accident ID"@en ;
  owl:equivalentProperty dcterms:identifier ;
  rdfs:domain safeschool:Accident ;
  rdfs:range xsd:string ;
  rdfs:comment "Unique expedition number for the accident"@en .

safeschool:occurrenceDate a owl:DatatypeProperty ;
  rdfs:label "occurrence date"@en ;
  rdfs:domain safeschool:Accident ;
  rdfs:range xsd:date ;
  rdfs:comment "Date when the accident occurred"@en .

safeschool:occurrenceTime a owl:DatatypeProperty ;
  rdfs:label "occurrence time"@en ;
  rdfs:domain safeschool:Accident ;
  rdfs:range xsd:time ;
  rdfs:comment "Time when the accident occurred"@en .

safeschool:accidentType a owl:DatatypeProperty ;
  rdfs:label "accident type"@en ;
  rdfs:domain safeschool:Accident ;
  rdfs:range xsd:string ;
  rdfs:comment "Type of accident"@en .

safeschool:weatherCondition a owl:DatatypeProperty ;
  rdfs:label "weather condition"@en ;
  rdfs:domain safeschool:Accident ;
  rdfs:range xsd:string ;
  rdfs:comment "Weather conditions at time of accident"@en .

safeschool:alcoholPositive a owl:DatatypeProperty ;
  rdfs:label "alcohol positive"@en ;
  rdfs:domain safeschool:Accident ;
  rdfs:range xsd:boolean ;
  rdfs:comment "Whether any alcohol test was positive"@en .

safeschool:drugPositive a owl:DatatypeProperty ;
  rdfs:label "drug positive"@en ;
  rdfs:domain safeschool:Accident ;
  rdfs:range xsd:boolean ;
  rdfs:comment "Whether any drug test was positive"@en .

safeschool:locationDescription a owl:DatatypeProperty ;
  rdfs:label "location description"@en ;
  rdfs:domain safeschool:Accident ;
  rdfs:range xsd:string ;
  rdfs:comment "Textual description of accident location"@en .


###########################################
# DATATYPE PROPERTIES - EDUCATIONAL CENTER
###########################################

safeschool:centerCode a owl:DatatypeProperty ;
  rdfs:label "center code"@en ;
  rdfs:domain safeschool:EducationalCenter ;
  rdfs:range xsd:string ;
  rdfs:comment "Unique 8-character center identifier"@en .

safeschool:centerName a owl:DatatypeProperty ;
  rdfs:label "center name"@en ;
  owl:equivalentProperty schema:name ;
  rdfs:domain safeschool:EducationalCenter ;
  rdfs:range xsd:string ;
  rdfs:comment "Name of the educational center"@en .

safeschool:centerType a owl:DatatypeProperty ;
  rdfs:label "center type"@en ;
  rdfs:domain safeschool:EducationalCenter ;
  rdfs:range xsd:string ;
  rdfs:comment "Type description of the educational center"@en .

safeschool:centerTypeCode a owl:DatatypeProperty ;
  rdfs:label "center type code"@en ;
  rdfs:domain safeschool:EducationalCenter ;
  rdfs:range xsd:string ;
  rdfs:comment "3-digit code for center type"@en .

safeschool:ownershipType a owl:DatatypeProperty ;
  rdfs:label "ownership type"@en ;
  rdfs:domain safeschool:EducationalCenter ;
  rdfs:range xsd:string ;
  rdfs:comment "Public or private ownership"@en .

safeschool:contactWebsite a owl:DatatypeProperty ;
  rdfs:label "contact website"@en ;
  owl:equivalentProperty schema:url ;
  rdfs:domain safeschool:EducationalCenter ;
  rdfs:range xsd:anyURI ;
  rdfs:comment "Website URL of the center"@en .

safeschool:contactEmail a owl:DatatypeProperty ;
  rdfs:label "contact email"@en ;
  owl:equivalentProperty schema:email ;
  rdfs:domain safeschool:EducationalCenter ;
  rdfs:range xsd:string ;
  rdfs:comment "Email contact for the center"@en .

safeschool:phone a owl:DatatypeProperty ;
  rdfs:label "phone"@en ;
  owl:equivalentProperty schema:telephone ;
  rdfs:domain safeschool:EducationalCenter ;
  rdfs:range xsd:string ;
  rdfs:comment "Phone number"@en .

safeschool:streetType a owl:DatatypeProperty ;
  rdfs:label "street type"@en ;
  rdfs:domain safeschool:EducationalCenter ;
  rdfs:range xsd:string ;
  rdfs:comment "Type of street"@en .

safeschool:streetName a owl:DatatypeProperty ;
  rdfs:label "street name"@en ;
  rdfs:domain safeschool:EducationalCenter ;
  rdfs:range xsd:string ;
  rdfs:comment "Name of the street."@en .

safeschool:postalCode a owl:DatatypeProperty ;
  rdfs:label "postal code"@en ;
  owl:equivalentProperty schema:postalCode ;
  rdfs:domain safeschool:EducationalCenter ;
  rdfs:range xsd:string ;
  rdfs:comment "Postal code"@en .

safeschool:districtCode a owl:DatatypeProperty ;
  rdfs:label "district code"@en ;
  rdfs:domain safeschool:EducationalCenter ;
  rdfs:range xsd:string ;
  rdfs:comment "2-digit district code for Madrid city centers"@en .

safeschool:districtName a owl:DatatypeProperty ;
  rdfs:label "district name"@en ;
  rdfs:domain safeschool:EducationalCenter ;
  rdfs:range xsd:string ;
  rdfs:comment "District name"@en .

###########################################
# DATATYPE PROPERTIES - LOCATION
###########################################

safeschool:latitude a owl:DatatypeProperty ;
  rdfs:label "latitude"@en ;
  owl:equivalentProperty wgs84:lat ;
  rdfs:domain safeschool:Location ;
  rdfs:range xsd:decimal ;
  rdfs:comment "Latitude coordinate"@en .

safeschool:longitude a owl:DatatypeProperty ;
  rdfs:label "longitude"@en ;
  owl:equivalentProperty wgs84:long ;
  rdfs:domain safeschool:Location ;
  rdfs:range xsd:decimal ;
  rdfs:comment "Longitude coordinate"@en .

safeschool:utmX a owl:DatatypeProperty ;
  rdfs:label "UTM X"@en ;
  rdfs:domain safeschool:Location ;
  rdfs:range xsd:integer ;
  rdfs:comment "UTM X coordinate"@en .

safeschool:utmY a owl:DatatypeProperty ;
  rdfs:label "UTM Y"@en ;
  rdfs:domain safeschool:Location ;
  rdfs:range xsd:integer ;
  rdfs:comment "UTM Y coordinate"@en .

###########################################
# DATATYPE PROPERTIES - SPEED CAMERA
###########################################

safeschool:cameraIdentifier a owl:DatatypeProperty ;
  rdfs:label "camera identifier"@en ;
  owl:equivalentProperty dcterms:identifier ;
  rdfs:domain safeschool:FixedSpeedCamera ;
  rdfs:range xsd:string ;
  rdfs:comment "Unique radar number"@en .

safeschool:cameraType a owl:DatatypeProperty ;
  rdfs:label "camera type"@en ;
  rdfs:domain safeschool:FixedSpeedCamera ;
  rdfs:range xsd:string ;
  rdfs:comment "Type of camera"@en .

safeschool:speedLimit a owl:DatatypeProperty ;
