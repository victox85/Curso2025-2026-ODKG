safeschool:trafficDirection a owl:DatatypeProperty ;
  rdfs:label "traffic direction"@en ;
  rdfs:domain safeschool:FixedSpeedCamera ;
  rdfs:range xsd:string ;
  rdfs:comment "Direction of monitored traffic"@en .

safeschool:lane a owl:DatatypeProperty ;
  rdfs:label "lane"@en ;
  rdfs:domain safeschool:FixedSpeedCamera ;
  rdfs:range xsd:string ;
  rdfs:comment "Lane specification"@en .

safeschool:responsibleAuthority a owl:DatatypeProperty ;
  rdfs:label "responsible authority"@en ;
  rdfs:domain safeschool:FixedSpeedCamera ;
  rdfs:range xsd:string ;
  rdfs:comment "Managing authority"@en .

###########################################
# DATATYPE PROPERTIES - INVOLVED PARTY
###########################################

safeschool:vehicleType a owl:DatatypeProperty ;
  rdfs:label "vehicle type"@en ;
  rdfs:domain safeschool:InvolvedParty ;
  rdfs:range xsd:string ;
  rdfs:comment "Type of vehicle"@en .

safeschool:personType a owl:DatatypeProperty ;
  rdfs:label "person type"@en ;
  rdfs:domain safeschool:InvolvedParty ;
  rdfs:range xsd:string ;
  rdfs:comment "Role in accident"@en .

safeschool:gender a owl:DatatypeProperty ;
  rdfs:label "gender"@en ;
  owl:equivalentProperty schema:gender ;
  rdfs:domain safeschool:InvolvedParty ;
  rdfs:range xsd:string ;
  rdfs:comment "Gender"@en .

safeschool:ageRange a owl:DatatypeProperty ;
  rdfs:label "age range"@en ;
  rdfs:domain safeschool:InvolvedParty ;
  rdfs:range xsd:string ;
  rdfs:comment "Age range"@en .

safeschool:injurySeverity a owl:DatatypeProperty ;
  rdfs:label "injury severity"@en ;
  rdfs:domain safeschool:InvolvedParty ;
  rdfs:range xsd:string ;
