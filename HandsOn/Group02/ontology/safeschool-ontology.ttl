safeschool:injurySeverityCode a owl:DatatypeProperty ;
  rdfs:label "injury severity code"@en ;
  rdfs:domain safeschool:InvolvedParty ;
  rdfs:range xsd:string ;
  rdfs:comment "Numeric code for injury severity"@en .