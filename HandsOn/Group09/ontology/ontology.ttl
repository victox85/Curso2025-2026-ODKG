#################################################################
#    Object Properties
#################################################################

###  https://linked.melbourne.example/ontology/urban-sensors#addressOfLocation
us:addressOfLocation rdf:type owl:ObjectProperty ;
                     owl:inverseOf us:hasAddress ;
                     rdfs:domain us:StreetAddress ;
                     rdfs:range us:SensorLocation ;
                     rdfs:comment "Inverse of hasAddress."@en ;
                     rdfs:label "address of location"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#appliesToArea
us:appliesToArea rdf:type owl:ObjectProperty ;
                 owl:inverseOf us:hasApplicableHoliday ;
                 rdfs:domain us:PublicHoliday ;
                 rdfs:range us:AdministrativeArea ;
                 rdfs:comment "The administrative area(s) where a public holiday is in force."@en ;
                 rdfs:label "applies to area"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#hasAddress
us:hasAddress rdf:type owl:ObjectProperty ;
              rdfs:domain us:SensorLocation ;
              rdfs:range us:StreetAddress ;
              rdfs:comment "Links a sensor location to its normalized street address."@en ;
              rdfs:label "has address"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#hasApplicableHoliday
us:hasApplicableHoliday rdf:type owl:ObjectProperty ;
                        rdfs:domain us:AdministrativeArea ;
                        rdfs:range us:PublicHoliday ;
                        rdfs:comment "Inverse of appliesToArea."@en ;
                        rdfs:label "has applicable holiday"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#hasObservation
us:hasObservation rdf:type owl:ObjectProperty ;
                  owl:inverseOf us:observedAtLocation ;
                  rdfs:domain us:SensorLocation ;
                  rdfs:range sosa:Observation ;
                  rdfs:comment "Inverse of observedAtLocation; a site aggregates observations."@en ;
                  rdfs:label "has observation"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#hasSite
us:hasSite rdf:type owl:ObjectProperty ;
           owl:inverseOf us:inAdministrativeArea ;
           rdfs:domain us:AdministrativeArea ;
           rdfs:range us:SensorLocation ;
           rdfs:comment "Inverse of inAdministrativeArea; the area has this site."@en ;
           rdfs:label "has site"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#holidayHasOccurrence
us:holidayHasOccurrence rdf:type owl:ObjectProperty ;
                        owl:inverseOf us:occursOnHoliday ;
                        rdfs:domain us:PublicHoliday ;
                        rdfs:range sosa:Observation ;
                        rdfs:comment "Inverse of occursOnHoliday."@en ;
                        rdfs:label "holiday has occurrence"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#hostsSensor
us:hostsSensor rdf:type owl:ObjectProperty ;
               owl:inverseOf us:installedAt ;
               rdfs:domain us:SensorLocation ;
               rdfs:range us:Sensor ;
               rdfs:comment "Relates a sensor location to a sensor that is/was installed there."@en ;
               rdfs:label "hosts sensor"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#inAdministrativeArea
us:inAdministrativeArea rdf:type owl:ObjectProperty ;
                        rdfs:domain us:SensorLocation ;
                        rdfs:range us:AdministrativeArea ;
                        rdfs:comment "Places a location within a city or state."@en ;
                        rdfs:label "in administrative area"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#installedAt
us:installedAt rdf:type owl:ObjectProperty ;
               rdfs:domain us:Sensor ;
               rdfs:range us:SensorLocation ;
               rdfs:comment "Relates a sensor device to the installation site (sensor location)."@en ;
               rdfs:label "installed at"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#observedAtLocation
us:observedAtLocation rdf:type owl:ObjectProperty ;
                      rdfs:domain sosa:Observation ;
                      rdfs:range us:SensorLocation ;
                      rdfs:comment "Links an observation to the sensor location where it was made."@en ;
                      rdfs:label "observed at location"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#occursOnHoliday
us:occursOnHoliday rdf:type owl:ObjectProperty ;
                   rdfs:domain sosa:Observation ;
                   rdfs:range us:PublicHoliday ;
                   rdfs:comment "Marks an observation as occurring on a specific public holiday."@en ;
                   rdfs:label "occurs on holiday"@en .


#################################################################
#    Data properties
#################################################################

###  https://linked.melbourne.example/ontology/urban-sensors#airTemperature
us:airTemperature rdf:type owl:DatatypeProperty ;
                  rdfs:domain us:MicroclimateObservation ;
                  rdfs:range xsd:decimal ;
                  rdfs:comment "Air temperature in degrees Celsius."@en ;
                  rdfs:label "air temperature (°C)"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#humidity
us:humidity rdf:type owl:DatatypeProperty ;
            rdfs:domain us:MicroclimateObservation ;
            rdfs:range xsd:decimal ;
            rdfs:comment "Relative humidity percentage."@en ;
            rdfs:label "relative humidity (%)"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#noise
us:noise rdf:type owl:DatatypeProperty ;
         rdfs:domain us:MicroclimateObservation ;
         rdfs:range xsd:decimal ;
         rdfs:comment "Acoustic noise level, decibels."@en ;
         rdfs:label "noise (dB)"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#pedestrianCount
us:pedestrianCount rdf:type owl:DatatypeProperty ;
                   rdfs:domain us:PedestrianObservation ;
                   rdfs:range xsd:integer ;
                   rdfs:comment "Total pedestrians counted during the observation interval."@en ;
                   rdfs:label "pedestrian count"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#pm10
us:pm10 rdf:type owl:DatatypeProperty ;
        rdfs:domain us:MicroclimateObservation ;
        rdfs:range xsd:decimal ;
        rdfs:comment "Mass concentration of particulate matter ≤10 µm."@en ;
        rdfs:label "PM10 concentration (µg/m³)"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#pm25
us:pm25 rdf:type owl:DatatypeProperty ;
        rdfs:domain us:MicroclimateObservation ;
        rdfs:range xsd:decimal ;
        rdfs:comment "Mass concentration of particulate matter ≤2.5 µm."@en ;
        rdfs:label "PM2.5 concentration (µg/m³)"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#postcode
us:postcode rdf:type owl:DatatypeProperty ;
            rdfs:domain us:StreetAddress ;
            rdfs:range xsd:string ;
            rdfs:comment "Postal code in the normalized address."@en ;
            rdfs:label "postcode"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#sensorId
us:sensorId rdf:type owl:DatatypeProperty ;
            rdfs:domain us:Sensor ;
            rdfs:range xsd:string ;
            rdfs:comment "Source identifier of a pedestrian or microclimate sensor."@en ;
            rdfs:label "sensor id"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#siteId
us:siteId rdf:type owl:DatatypeProperty ;
          rdfs:domain us:SensorLocation ;
          rdfs:range xsd:string ;
          rdfs:comment "Identifier of a sensor installation site (e.g., location_id, site_id)."@en ;
          rdfs:label "site id"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#streetName
us:streetName rdf:type owl:DatatypeProperty ;
              rdfs:domain us:StreetAddress ;
              rdfs:range xsd:string ;
              rdfs:comment "Name of the street in the normalized address."@en ;
              rdfs:label "street name"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#streetNumber
us:streetNumber rdf:type owl:DatatypeProperty ;
                rdfs:domain us:StreetAddress ;
                rdfs:range xsd:string ;
                rdfs:comment "Street number in the normalized address."@en ;
                rdfs:label "street number"@en .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/ns/sosa/Observation
sosa:Observation rdf:type owl:Class .


###  https://linked.melbourne.example/ontology/urban-sensors#AdministrativeArea
us:AdministrativeArea rdf:type owl:Class ;
                      rdfs:comment "An administrative unit such as a city or state."@en ;
                      rdfs:label "Administrative area"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#City
us:City rdf:type owl:Class ;
        rdfs:subClassOf us:AdministrativeArea ;
        owl:disjointWith us:State ;
        rdfs:comment "City-level administrative area (e.g., City of Melbourne)."@en ;
        rdfs:label "City"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#MicroclimateObservation
us:MicroclimateObservation rdf:type owl:Class ;
                           rdfs:subClassOf sosa:Observation ;
                           owl:disjointWith us:PedestrianObservation ;
                           rdfs:comment "Environmental reading (e.g., temperature, humidity) from a microclimate device."@en ;
                           rdfs:label "Microclimate observation"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#MicroclimateSensor
us:MicroclimateSensor rdf:type owl:Class ;
                      rdfs:subClassOf us:Sensor ;
                      owl:disjointWith us:PedestrianSensor ;
                      rdfs:comment "A device that measures local environmental conditions (air temperature, humidity, particulate matter, etc.)."@en ;
                      rdfs:label "Microclimate sensor"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#PedestrianObservation
us:PedestrianObservation rdf:type owl:Class ;
                         rdfs:subClassOf sosa:Observation ;
                         rdfs:comment "Hourly pedestrian count observation."@en ;
                         rdfs:label "Pedestrian observation"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#PedestrianSensor
us:PedestrianSensor rdf:type owl:Class ;
                    rdfs:subClassOf us:Sensor ;
                    rdfs:comment "A sensor device counting pedestrians (City of Melbourne Pedestrian Counting System)."@en ;
                    rdfs:label "Pedestrian sensor"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#PublicHoliday
us:PublicHoliday rdf:type owl:Class ;
                 rdfs:comment "A public holiday that can apply to one or more administrative areas and dates."@en ;
                 rdfs:label "Public holiday"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#Sensor
us:Sensor rdf:type owl:Class ;
          rdfs:comment "Abstract sensor device in the project (pedestrian or microclimate)."@en ;
          rdfs:label "Sensor"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#SensorLocation
us:SensorLocation rdf:type owl:Class ;
                  rdfs:comment "A concrete installation site of a sensor device (enriched with address and coordinates)."@en ;
                  rdfs:label "Sensor location"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#State
us:State rdf:type owl:Class ;
         rdfs:subClassOf us:AdministrativeArea ;
         rdfs:comment "State-level administrative area (e.g., Victoria)."@en ;
         rdfs:label "State"@en .


###  https://linked.melbourne.example/ontology/urban-sensors#StreetAddress
us:StreetAddress rdf:type owl:Class ;
                 rdfs:comment "Normalized address data (street name, number, postcode)."@en ;
                 rdfs:label "Street address"@en .


