@prefix ns: <http://www.barnabikes.org/ODKG/handsOn/group10/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix mod: <https://w3id.org/mod#> .

# Ontology metadata
<http://www.barnabikes.org/ODKG/handsOn/group10/ontology> a owl:Ontology ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

# ---------- CLASSES ----------
ns:Bar a owl:Class ;
    rdfs:label "Bar" .

ns:BikingStation a owl:Class ;
    rdfs:label "Biking Station" .

ns:District a owl:Class ;
    rdfs:label "District" .

ns:Street a owl:Class ;
    rdfs:label "Street" .

# ---------- OBJECT PROPERTIES ----------
ns:hasDistrict a owl:ObjectProperty ;
    rdfs:label "has district" ;
    rdfs:domain ns:BikingStation ;
    rdfs:range ns:District .

ns:hasStreet a owl:ObjectProperty ;
    rdfs:label "has street" ;
    rdfs:domain ns:BikingStation ;
    rdfs:range ns:Street .

ns:locatedIn a owl:ObjectProperty ;
    rdfs:label "located in" ;
    rdfs:domain ns:Bar ;
    rdfs:range ns:District .

ns:onStreet a owl:ObjectProperty ;
    rdfs:label "on street" ;
    rdfs:domain ns:Bar ;
    rdfs:range ns:Street .

# ---------- DATATYPE PROPERTY ----------
ns:name a owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:domain ns:District ;
    rdfs:range xsd:string .
